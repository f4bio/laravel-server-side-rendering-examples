{
  "private": true,
  "scripts": {
    "lint": "npm run lint:styles && npm run lint:html && npm run lint:php",
    "lint:html": "echo 'npx eslint --ext .html ./resources'",
    "lint:styles": "npx stylelint ./resources/styles/**/*.scss",
    "lint:styles:fix": "npx stylelint ./resources/styles/**/*.css --fix",
    "lint:php": "npx eslint --ext .blade.php ./resources",
    "clear": "npx rimraf ./public/fonts ./public/images ./public/*{css,png,ico,manifest,svg,json,xml}",
    "build": "npm run prod",
    "build:prod": "npm run prod",
    "build:dev": "npm run dev",
    "dev": "npm run development",
    "prod": "npm run production",
    "development": "npx mix",
    "watch": "npx mix watch",
    "watch:poll": "npx mix watch -- --watch-options-poll=1000",
    "hot": "npx mix watch --hot",
    "production": "npx mix --production"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.16.7",
    "@fontsource/fira-sans": "4.5.3",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.4.0",
    "@tailwindcss/line-clamp": "0.3.1",
    "@tailwindcss/typography": "0.5.2",
    "autoprefixer": "10.4.2",
    "axios": "^0.26.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "10.2.4",
    "eslint": "8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-php-markup": "6.0.0",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-unused-imports": "2.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "glob-all": "^3.2.1",
    "laravel-mix": "6.0.43",
    "laravel-mix-clean": "0.1.0",
    "laravel-mix-merge-manifest": "2.0.0",
    "laravel-mix-purgecss": "6.0.0",
    "lodash": "^4.17.21",
    "postcss": "8.4.7",
    "postcss-css-variables": "0.18.0",
    "postcss-import": "14.0.2",
    "postcss-preset-env": "7.4.1",
    "postcss-url": "10.1.3",
    "prettier": "^2.5.1",
    "purgecss-webpack-plugin": "^4.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "resolve-url-loader": "5.0.0",
    "stylelint": "14.5.3",
    "stylelint-config-recommended": "7.0.0",
    "tailwindcss": "3.0.23",
    "vue": "^3.2.31",
    "vue-router": "^4.0.13",
    "vue-server-renderer": "^2.6.14",
    "vuex": "^4.0.2",
    "webpack": "5.69.1"
  }
